<p id="notice"><%= notice %></p>
<div class="col-sm-2">
  <% if logged_in? %>
      <h5>Chatroom:</h5>
      <ul>
        <% current_user.chatrooms.each do |chatroom| %>
            <li><%= chatroom.name%></li>
        <% end %>
      </ul>

  <% end %>
</div>
<p>
  <strong>Name:</strong>
  <%= @chatroom.name %>
</p>

<%= link_to 'Edit', edit_chatroom_path(@chatroom) %> |
<%= link_to 'Back', chatrooms_path %>
